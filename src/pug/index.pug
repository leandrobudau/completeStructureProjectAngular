doctype html
html(lang="pt-br" ng-app="app")
	head
		title Titulo do site
		meta(charset="UTF-8")
		meta(name="viewport" content="width=device-width, initial-scale=1")
		link(rel="stylesheet", href="https://fonts.googleapis.com/icon?family=Material+Icons")
		//build:css assets/css/libs/font-awesome.css
		link(href="../bower_components/font-awesome/css/font-awesome.css" rel="stylesheet")
		//endbuild
		//build:css assets/css/main.css
		link(href="assets/css/main.css" rel="stylesheet")  
		//endbuild
	body
		include includes/header.pug
		include includes/nav.pug
		ui-view
		include includes/footer.pug
		//build:js assets/js/libs/angular.js
		script(src="../bower_components/angular/angular.js")
		script(src="../bower_components/angular-ui-router/release/angular-ui-router.js")
		script(src="../bower_components/angular-resource/angular-resource.js")
		//endbuild
		//build:jsNgAnnotate default.js
		script(src="app.js")
		script(src="config.js")
		script(src="router.js")
		script(src="factory/statusCodeHttpFactory.js") 
		script(src="factory/user/userFactory.js")
		script(src="interceptors/requisitionInterceptor.js") 
		script(src="directives/fileModelDirective.js") 
		script(src="controllers/errorHttpRequestController.js") 
		script(src="controllers/headerController.js")
		script(src="controllers/user/loginUserController.js") 
		script(src="controllers/user/registerUserController.js")
		//Components
		script(src="components/sidebar/baSidebar.service.js")
		script(src="components/sidebar/baSidebar.directive.js")
		script(src="components/sidebar/baSidebarHelpers.directive.js") 
		script(src="components/sidebar/baSidebarCtrl.js")
		//endbuild